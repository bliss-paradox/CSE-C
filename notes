git init
git config --global user.name "Anushka"
git config --global user.email "anuad1010@gmail.com"
git remote add origin https://github.com/bliss-paradox/CSE-C.git #linking repo to vs code
git add . # to send it in the staging area
git status
git restore DAY2.html #to move back from the staging area
git commit -m "this is my version1" #to commit the code
git branch -M main #adding main branch to repo
git push -u origin main
git branch Anushka #adding sub branches in the repo
git push --set-upstream origin Anushka

